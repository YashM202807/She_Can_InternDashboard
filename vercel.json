{
  "version": 2,
  "builds": [
    { "src": "api/login.js", "use": "@vercel/node" },
    { "src": "api/interns.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/login", "dest": "api/login.js" },
    { "src": "/api/interns", "dest": "api/interns.js" },
    { "src": "/api/suggest-referral", "dest": "api/interns.js" },
    { "src": "/api/interns/(.*)", "dest": "api/interns.js" },
    { "src": "/(.*)", "dest": "/public/index.html" }
  ]
}
